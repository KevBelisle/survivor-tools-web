{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.5.1",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@microsoft/applicationinsights-react-js": "^3.1.2",
    "@microsoft/applicationinsights-web": "^2.6.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.21.1",
    "chart.js": "^3.2.1",
    "chartjs-adapter-date-fns": "^1.1.0-beta.1",
    "date-fns": "^2.21.3",
    "framer-motion": "^4.1.4",
    "fuse.js": "^6.4.6",
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-query": "^3.15.2",
    "react-responsive-masonry": "^2.1.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "simple-react-lightbox": "^3.6.6",
    "use-is-in-viewport": "^1.0.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "react-scripts start",
    "start:prod": "REACT_APP_API_BASE_URL=https://api.survivor.tools react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint \"./src/**/*.js\" --fix"
  },
  "eslintConfig": {
    "plugins": [
      "import"
    ],
    "extends": [
      "react-app",
      "plugin:import/recommended"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "import/named": "warn",
      "import/namespace": "warn",
      "import/no-named-as-default": "off",
      "import/export": "warn",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "ignore",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
